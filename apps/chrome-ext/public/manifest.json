{
  "name": "ChatGPT Github Sync",
  "version": "0.1.1",
  "manifest_version": 3,
  "description": " Github Sync",
  "icons": {
    "128": "icon_128.png"
  },
  "action": {
    "default_title": "",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/chat*",
        "https://sharegpt.com/c/*"
      ],
      "js": [
        "assets/content_script.js"
      ]
    }
  ]
}